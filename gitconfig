[color]
  ui = true

[color "status"]
  added = green
  changed = red bold
  untracked = magenta bold

[color "branch"]
  remote = yellow

[push]
	default = current
[core]
	editor = /usr/bin/vim
	excludefile = ~/.gitignore
	excludesfile = /Users/Sean/.gitignore

[alias]
  st = status
  co = commit
  br = branch
  ch = checkout
  d = diff
  p = push
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[credential]
	helper = osxkeychain
