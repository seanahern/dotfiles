[user]
	name = Sean Ahern
	email = seanpahern@gmail.com

[color]
  ui = true

[color "status"]
  added = green
  changed = red bold
  untracked = magenta bold

[color "branch"]
  remote = yellow

[push]
	default = current
[core]
	editor = /usr/bin/vim
	excludefile = ~/.gitignore
	excludesfile = /Users/Sean/.gitignore

[alias]
  st = status
  co = commit
  br = branch
  ch = checkout
  d = diff
  p = push
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
